{
  "name": "sogni-image-restore-superapp",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npx kill-port 5173 && vite --host 0.0.0.0 --port 5173 --strictPort",
    "build": "tsc && vite build",
    "preview": "vite preview",
